<?xml version = "1.0" encoding = "ISO-8859-1"?><!DOCTYPE aplicacion SYSTEM "dtds/aplicacion.dtd"><aplicacion nombre="ConsultorioJuridico"> 		<!--	Un m�dulo por defecto se asume para cada	controlador en <modulo-defecto/> 	-->	<modulo-defecto>		  		<controlador nombre="Typical"/>			</modulo-defecto>		<!--	Podemos definir m�dulos adicionales, por ejemplo: 		<modulo nombre="ProfesorSoloLectura">		<modelo nombre="Profesor"/>		<controlador nombre="Print"/>	</modulo>			-->	  		<modulo nombre="AgendaRequest">	    <var-entorno nombre="XAVA_SEARCH_ACTION" valor="AgendaRequest.searchAgenda"/>	    <modelo nombre="AgendaRequest"/>	    <controlador nombre="Typical"/>	    <controlador nombre="AgendaRequest"/>	</modulo>		<modulo nombre="TodayAgendaRequest">	    <var-entorno nombre="XAVA_SEARCH_ACTION" valor="AgendaRequest.searchAgenda"/>	    <modelo nombre="AgendaRequest"/>	    <controlador nombre="Typical"/>	    <controlador nombre="TodayAgendaRequest"/>	</modulo>		<modulo nombre="CaseFileModule">		<modelo nombre="CaseFile"/>		<controlador nombre="CaseFileEdition"/>		<controlador-modo nombre="Void"/> 	</modulo></aplicacion>