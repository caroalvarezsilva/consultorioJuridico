<?xml version = "1.0" encoding = "ISO-8859-1"?><!DOCTYPE application SYSTEM "dtds/application.dtd"><application name="ConsultorioJuridico"> 		<!-- 	A default module for each entity is assumed with the	controllers on <default-module/>	-->	<default-module>		  		<controller name="Typical"/>			</default-module>		<!-- 	You can define additional modules, for example:	  	<module name="OnlyReadTeacher">		<model name="Teacher"/>		<controller name="Print"/>	</module>			-->	<module name="AgendaRequestModule">		<model name="AgendaRequest"/>		<controller name="CreateCaseFile"/>	</module>		<module name="CaseFileModule">		<model name="CaseFile"/>		<controller name="CaseFileEdition"/>		<controller-mode name="Void"/> <!-- Así el módulo tiene sólo modo de detalle -->	</module>	</application>